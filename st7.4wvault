---------------------------------------------------------------------Start script--------------------------------------------------------------
#!/bin/sh
echo "start jboss for TFASG EAP74"

export JAVA_HOME=/app/java/jdk11
export PATH=$JAVA_HOME/bin:$PATH

INSTANCEPATH=/prodlib/TFASG
JBOSS_HOME=/app/jboss/eap74/TFASG
JBOSS_MASTER_IP=`hostname`
JBOSS_DOMAIN_LOG_DIR=${INSTANCEPATH}/logs/eap74
JBOSS_DOMAIN_CONFIG_DIR=${JBOSS_HOME}/standalone/configuration
JBOSS_DOMAIN_BASE_DIR=${JBOSS_HOME}/standalone

export JAVA_OPTS="${JAVA_OPTS} -Dbatchloader.firware.deleteAfterSuccess=true -Dvtrack.http.channel=http -Dspring.profiles.active=default,Oracle,Oracle11g"
export JAVA_OPTS="${JAVA_OPTS} -Dcountry=SG -Dvtrack.log4j.dir=/prodlib/TFASG/logs/vtrack -Dprov.log4j.dir=/prodlib/TFASG/logs/provision -Dprov.log4j.debug -Dvtap.log4j.debug -Dvtap.log4j.dir=/prodlib/TFASG/logs/vtap -Dbatchloader.log4j.dir=/prodlib/TFASG/logs/"
export JAVA_OPTS="${JAVA_OPTS} -Dbatchloader.args.customerid=1032 -Dbatchloader.firmware.dir=/tmp/TFASG/"
export JAVA_OPTS="${JAVA_OPTS} -Dconsumercomponents.log4j.dir=/prodlib/TFASG/logs/consumercomponents -Dmessagequeue.jms.enable=false -Dmessagequeue.jms.username=Jmsuser -Dmessagequeue.jms.password=password -Dmessagequeue.jms.errorrecordfile=$JBOSS_HOME/syslog/SyslogErrorRecord.dat -Dsoap.external.url=http://lxtfatsgv095:8088/mockHTTPEndpointbinding?WSDL -Dmessagequeue.jms.port=4447 -Dmessagequeue.jms.host=$1 -Dsoap.reprocess.dir=$JBOSS_HOME/reprocess/ -Dorg.jboss.as.logging.per-deployment=false -Dvtap.temp.dir=$JBOSS_HOME/temp -Dvtap.host=lxtfatsgv095 -Dvtap.port=9080 -Dprovision.host=lxtfatsgv095 -Dprovision.port=9080 -Dprovision.protocol=http -Dvtap.protocol=http -Dvtap.lock.timeout.hours=1 -Dvguard.profile.oatupdates.enable=true -DmaxDaysLogsToKeep=60"


nohup sh ${JBOSS_HOME}/bin/standalone.sh -DconfigDir=/prodlib/TFASG/conf -Djboss.socket.binding.port-offset=1000 --properties=/prodlib/TFASG/conf/config.properties -Djboss.bind.address=`hostname` -Djboss.bind.address.management=`hostname` -Djava.security.egd=file:///dev/urandom -DprintExceptionLog=Y -Dfile.encoding=utf-8  -Djboss.domain.base.dir=${JBOSS_DOMAIN_BASE_DIR} -Djboss.domain.config.dir=${JBOSS_DOMAIN_CONFIG_DIR} -Djboss.domain.log.dir=${JBOSS_DOMAIN_LOG_DIR} -Doracle.net.tns_admin=/app/oraclient/product/19.3.0.0/client_1/network/admin/ --server-config=standalone-ha.xml -b lxtfatsgv095 > /dev/null 2>&1 < /dev/null &

------------------------------------------------------------Stop script --------------------------------------------------------------------------------------
#!/bin/sh
echo "stop jboss for TFASG EAP74"
ps -ef|grep jboss |grep "/app/jboss/eap74/TFASG/" |grep -v grep| grep -v stopLXTFATSGV095.sh |awk -F' ' '{print $2}' |xargs  --no-run-if-empty kill -9

